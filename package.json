{
  "name": "cf-helper",
  "version": "1.0.0",
  "description": "cloud foundry scraping scripts",
  "license": "MIT",
  "repository": "",
  "main": "./lib/main-module.js",
  "contributors": [
    {
      "name": "Sean Kemmis",
      "email": "squatchy86@gmail.com"
    }
  ],
  "scripts": {
    "build": "npm run compile",
    "compile": "BABEL_ENV=script babel src --out-dir lib",
    "go": "npm run compile && node lib/main-module.js",
    "lint": "eslint --ext .js,.jsx src test",
    "test": "npm run lint && npm run test:only",
    "test:only": "BABEL_ENV=test jest",
    "test:watch": "BABEL_ENV=test jest --watch",
    "test:watchAll": "BABEL_ENV=test jest --watchAll"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.js?$",
    "testPathIgnorePatterns": [
      "./test/makeTestData.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cpx": "^1.3.1",
    "cross-env": "~1.0.7",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-wyze": "3.1.0",
    "jest": "^21.2.1",
    "regenerator-runtime": "^0.11.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "child-process-promise": "^2.2.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0"
  },
  "bin": {
    "cf-helper": "./lib/main-module.js"
  }
}
